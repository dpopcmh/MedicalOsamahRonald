<Window x:Class="MedicalProject.PatientRecordDiagnosis"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MedicalProject"
        mc:Ignorable="d"
        Title="Add Diagnosis" Height="190" Width="300">
    <Grid>
        <StackPanel Grid.Column="0">
            <Button x:Name="okButton" Click="okButton_Click" IsDefault="True" RenderTransformOrigin="0.502,6.021" Content="OK" Margin="0,0,191,0"/>
            <Button x:Name="cancelButton" IsCancel="True" Content="Cancel" Margin="0,0,191,0" Click="cancelButton_Click"/>
        </StackPanel>
    </Grid>
</Window>
